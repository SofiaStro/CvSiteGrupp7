@model IEnumerable<Data.Models.UsersInProjects>

@{
    ViewBag.Title = "UsersInProjectIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Dessa användare är involverade i projektet: </h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.ActionLink("Visa CV", "Index", new { id = item.UserId })
        </td>
    </tr>
}

</table>
